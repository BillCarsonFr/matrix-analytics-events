{
  "type": "object",
  "description": "Triggered when the user changed screen",
  "properties": {
    "eventName": {
      "enum": ["Screen"]
    },
    "screenName": {
      "type": "string",
      "enum": [
        "Home",
        "Room",
        "RoomDirectory",
        "User",
        "Group",
        "MyGroups",
        "WebLoading",
        "WebWelcome",
        "WebLogin",
        "WebRegister",
        "WebForgotPassword",
        "WebCompleteSecurity",
        "WebE2ESetup",
        "WebSoftLogout"
      ]
    },
    "durationMs": {
      "description": "How long the screen was displayed for in milliseconds.",
      "type": "integer"
    }
  },
  "required": ["screenName", "eventName"],
  "additionalProperties": false
}
