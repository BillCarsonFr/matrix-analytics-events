{
  "type": "object",
  "description": "Used to track the performance of sync operations.",
  "properties": {
    "eventName": {
      "enum": ["SyncPerformance"]
    },
    "operation": {
      "description": "The operation that was profiled.",
      "type": "string",
      "enum": [
        "InitialSyncRequest",
        "InitialSyncParsing"
      ]
    },
    "durationMs": {
      "description": "The length of the operation in milliseconds.",
      "type": "number"
    },
    "context": {
      "description": "Client defined, can be used for debugging.",
      "type": "string"
    }
  },
  "required": ["operation", "durationMs", "eventName"],
  "additionalProperties": false
}
