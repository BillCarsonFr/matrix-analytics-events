{
  "type": "object",
  "description": "Triggered when an error occurred",
  "properties": {
    "eventName": {
      "title": "ErrorEventName",
      "enum": ["Error"]
    },
    "domain": {
      "type": "string",
      "title": "ErrorDomain",
      "enum": [
        "E2EE",
        "VOIP"
      ]
    },
    "name": {
      "type": "string",
      "title": "ErrorName",
      "enum": [
        "UnknownError",
        "OlmIndexError",
        "OlmKeysNotSentError",
        "OlmUnspecifiedError",
        "VoipUserHangup",
        "VoipIceFailed",
        "VoipInviteTimeout",
        "VoipIceTimeout",
        "VoipUserMediaFailed"
      ],
      "required": true
    },
    "context": {
      "description": "Context - client defined, can be used for debugging",
      "type": "string"
    }
  },
  "required": ["domain", "name", "eventName"],
  "additionalProperties": false
}
